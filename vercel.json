{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/docs",
            "dest": "/api/docs"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 60
        }
    }
}